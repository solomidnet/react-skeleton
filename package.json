{
  "name": "@solo.gg/react-skeleton",
  "version": "0.0.7",
  "description": "⚡⚡⚡ Full fledged flexible skeleton loader for styled-components ⚡⚡⚡",
  "main": "lib/index.js",
  "homepage": "http://solo-gg-react-skeleton.now.sh",
  "scripts": {
    "dev": "next",
    "test": "jest",
    "clean": "rimraf lib",
    "prereleasepkg": "npm run clean",
    "releasepkg": "babel src --out-dir lib",
    "build": "next build",
    "start": "next start",
    "prepublish": "npm run release && npm run test",
    "test:watch": "jest --watch",
    "deploy": "now && now alias"
  },
  "keywords": [],
  "author": "solo.gg",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@mdx-js/mdx": "0.15.2",
    "@mdx-js/tag": "0.15.0",
    "@taskr/babel": "1.1.0-next.1",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/shell": "1.1.0",
    "@taskr/watch": "1.1.0",
    "@zeit/next-mdx": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "1.7.1",
    "babel-preset-imbhargav5": "2.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.0",
    "jsdom": "12.0.0",
    "next": "7.0.0-canary.18",
    "np": "3.0.4",
    "prismjs": "1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-live": "1.11.0",
    "react-test-renderer": "16.5.1",
    "rimraf": "2.6.2",
    "styled-components": "4.0.0-beta.4-hmr",
    "taskr": "1.1.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testURL": "http://localhost",
    "clearMocks": true,
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/__tests__/"
    ],
    "setupFiles": [
      "<rootDir>/src/test/enzymeSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/pages"
    ]
  },
  "peerDependencies": {
    "styled-components": "beta",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solomidnet/react-skeleton.git"
  },
  "now": {
    "name": "react-skeleton",
    "alias": "solo-gg-react-skeleton",
    "type": "docker",
    "public": true
  }
}
